                    <a href="/logout" class="nav-link" aria-label="Logout button">
                        <svg viewBox="0 0 24 24" class="logout-btn" width="22px" height="22px" fill="none"
                            xmlns="http://www.w3.org/2000/svg" stroke="#0D4A4A">
                            <g stroke-width="0"></g>
                            <g stroke-linecap="round" stroke-linejoin="round"></g>
                            <path d="M21 12L13 12" stroke="#0D4A4A" stroke-width="2" stroke-linecap="round"
                                stroke-linejoin="round"></path>
                            <path d="M18 15L20.913 12.087V12.087C20.961 12.039 20.961 11.961 20.913 11.913V11.913L18 9"
                                stroke="#0D4A4A" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
