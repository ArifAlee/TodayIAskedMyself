<header>
    <div class="p-3">
        <a href="/" class="nav-link float-start" aria-current="page">TIAM</a>
        <nav class="nav nav-masthead justify-content-center mb-5 float-end">
            <div>
                <a href="/feed" class="nav-link feed-btn" aria-label="Feed button">
                    <!-- feed svg -->
                    <svg xmlns="http://www.w3.org/2000/svg" width="24px" height="24px" fill="#0D4A4A"
                        viewBox="0 0 256 256">
                        <path
