                <a href="/login" class="nav-link">login</a>
                <a href="/register" class="nav-link">register</a>
                <% } else { %>
                    <div class="">
                        <a href="/user/<%=currentUser.username%>/journal" class="nav-link journal-btn"
                            aria-label="Journal button">
                            <svg viewBox="0 0 16 16" xmlns="http://www.w3.org/2000/svg" height="19px" width="20px" fill="#0d4a4a" stroke="#0d4a4a"
                                stroke-width="0.4">
                                <path fill-rule="evenodd"
                                    d="M6 8V1h1v6.117L8.743 6.07a.5.5 0 0 1 .514 0L11 7.117V1h1v7a.5.5 0 0 1-.757.429L9 7.083 6.757 8.43A.5.5 0 0 1 6 8z">
